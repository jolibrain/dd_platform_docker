{
  "common": {
    "name": "DeepDetect"
  },
  "homeComponent": {
    "contentType": "json",
    "title": "DeepDetect Platform",
    "description": [
      "Welcome to deepdetect"
    ],
    "headerLinks": {
      "linkJupyter": "/code/lab"
    }
  },
  "deepdetect": {
    "refreshRate": {
      "info": 10000,
      "training": 30000
    },
    "servers": [
      {
        "name": "deepdetect",
        "settings": {
          "path": "/api/deepdetect",
          "isHostable": true,
          "isWritable": true
        }
      }
    ],
    "services": {
      "defaultConfig": [
        {
          "modelName": "default",
          "modelConfig": {
            "description": "",
            "model": {
              "repository": ""
            },
            "mllib": "caffe",
            "type": "supervised",
            "parameters": {
              "input": {
                "connector": "image",
                "height": 300,
                "width": 300
              },
              "mllib": {
                "nclasses": 2,
                "gpu": true,
                "gpuid": 0
              }
            }
          }
        }
      ]
    }
  },
  "imaginate": {
    "default": {
      "display": {
        "initImages": {},
        "mode": "description",
        "okClass": "",
        "boundingBox": true,
        "segmentation": false,
        "separateSegmentation": false,
        "segmentationColors": ["#1b9e77", "#d95f02"]
      },
      "threshold": {
        "confidence": 0.5,
        "controls": true,
        "controlSteps": [0.8, 0.41, 0.1]
      },
      "request": {
        "objSearch": false,
        "imgSearch": false,
        "best": false,
        "ctc": false,
        "blank_label": false
      }
    },
    "services": [
      {
        "name": "testService",
        "settings": {
          "display": {
            "initImages": {},
            "mode": "description",
            "okClass": "",
            "boundingBox": true,
            "segmentation": false,
            "separateSegmentation": false,
            "segmentationColors": ["#1b9e77", "#d95f02"]
          },
          "threshold": {
            "confidence": 0.5,
            "controls": true,
            "controlSteps": [0.8, 0.5, 0.3]
          },
          "request": {
            "objSearch": false,
            "imgSearch": false,
            "best": false,
            "ctc": false,
            "blank_label": false
          }
        }
      }
    ]
  },
  "gpuInfo": {
    "servers": [
      {
        "name": "GPU Stats",
        "url": "/gpu/stats"
      }
    ],
    "refreshRate": 5000
  },
  "modelRepositories": [
    {
      "name": "public",
      "nginxPath": "/models/public/",
      "systemPath": "/opt/platform"
    },
    {
      "name": "private",
      "nginxPath": "/models/private/",
      "systemPath": "/opt/platform",
      "hasFiles": true
    },
    {
      "name": "training",
      "nginxPath": "/models/training/",
      "systemPath": "/opt/platform",
      "isTraining": true
    }
  ],
  "dataRepositories": {
    "nginxPath": "/data/",
    "systemPath": "/opt/platform"
  },
  "modals": ["deletePredictService", "stopTraining"],
  "componentBlacklist": ["LinkChat"]
}
